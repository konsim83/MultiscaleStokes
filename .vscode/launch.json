  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) MsStokes 2D",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../build-vs-${workspaceFolderBasename}/source/MsStokes",
      "args": [
        "-p",
        "${workspaceFolder}/data/2D-test-01.prm"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/../build-vs-${workspaceFolderBasename}/",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) MsStokes 3D",
